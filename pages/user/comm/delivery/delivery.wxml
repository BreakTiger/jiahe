            <image src="/images/1teacher/h_icon3@2x.png"></image>
            <text class='job_three_txt'>{{jobData.education}}</text>
        </view>
    </view>


    <view class='job_four'>
             <image src="{{jobData.principal.principalimage}}"></image>
             <text class='job_four_txt'>{{jobData.principal.principalname}}</text>
             <view class='job_four_commun'>立即沟通</view> 
    </view>
  
